{
  "extends": "./tsconfig.app.json",
  "include": [
    "src/**/__tests__/*",
    "src/**/__tests__/**/*.spec.ts",
    "src/**/__tests__/**/*.test.ts",
    "env.d.ts"
  ],
  "exclude": [],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "types": [
      "vitest/globals",
      "node",
      "jsdom"
    ],
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "noEmit": true
  }
}